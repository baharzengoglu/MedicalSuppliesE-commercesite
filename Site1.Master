                <br />
                <img src="images/slogan.JPG" /></div>
            <div class="diviletisim">
                <br />
                <br />
                <br />
                <br />
                <asp:Label ID="Label1" runat="server" Text="Tel : (212) 544 44 44"></asp:Label>
            </div>
        </div>
        <div id="ustmenu">Üst Buton Menü</div>
        <div id="solmenu">Kategoriler<br />
